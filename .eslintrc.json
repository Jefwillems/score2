{
  "root": true,
  "env": {
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:vue/recommended"],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "no-console": "off",
    "quotes": [
      "error",
      "single",
      {
        "allowTemplateLiterals": true
      }
    ]
  }
}
